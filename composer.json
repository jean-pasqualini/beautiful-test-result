{
    "name": "darkilliant/beautiful-test-result",
    "description": "Generate onefile for support emoticon in behat/phpunit",
    "type": "library",
    "keywords": [
        "emoticone",
        "phpunit"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Pasqualini",
            "email": "jpasqualini75@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "elvanto/litemoji": "^1.1",
        "php-school/psx": "^1.1",
        "phpunit/phpunit": "^5.7||^6.0",
        "symfony/console": "^3.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        },
        "sort-packages": true
    }
}
